    "typeface-dosis": "^1.1.13",
    "validator": "^12.2.0",
    "vue-echarts": "^5.0.0-beta.0",
    "vue-gettext": "^2.1.12",
    "vue-good-table": "^2.21.2",
    "vue-pluralize": "^0.0.2"
  },
  "devDependencies": {
    "@quasar/app": "^2.1.14",
    "@quasar/quasar-app-extension-dotenv": "^1.0.5",
    "babel-eslint": "^10.1.0",
    "easygettext": "^2.16.1",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.15.0",

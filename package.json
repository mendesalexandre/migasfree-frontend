    "leaflet": "^1.7.1",
    "performant-array-to-tree": "^1.8.1",
    "quasar": "^1.15.10",
    "text-clipper": "^2.1.0",
    "typeface-dosis": "^1.1.13",
    "validator": "^13.5.2",
    "vue-echarts": "^6.0.0-rc.4",
    "vue-gettext": "^2.1.12",
    "vue-good-table": "^2.21.9",
